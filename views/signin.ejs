    <!--eboi interface-->
    <main class="d-flex flex-fill justify-content-center align-items-center py-5" data-bs-theme="light">
        <form action="" class="card rounded-3 shadow p-5 bg-body">
            <div class="d-flex justify-content-center mb-3">
                <img src="assets/eboi-logo.png" style="width: 10rem;">
            </div>

            <div class="d-flex flex-column gap-3">
                <div>
                    <label for="name" class="form-label">Nome</label>
                    <input type="text" class="form-control" id="name" name="name">
                </div>
                <div>
                    <label for="birth" class="form-label">Nascimento</label>
                    <input type="date" class="form-control" id="birth" name="birth">
                </div>
                <div>
                    <label for="cpf" class="form-label">CPF</label>
                    <input type="text" class="form-control" id="cpf" name="cpf" placeholder="000.000.000-00" maxlength="14">
                </div>
                <div>
                    <label for="rg" class="form-label">RG</label>
                    <input type="text" class="form-control" id="rg" name="rg" placeholder="0000000-0" maxlength="9">
                </div>
                <div>
                    <label for="email" class="form-label">Email</label>
                    <input type="email" class="form-control" id="email" name="email" placeholder="exemplo@exemplo.com">
                </div>
                <div>
                    <label for="password" class="form-label">Senha</label>
                    <input type="password" class="form-control" id="password" name="password">
                </div>
                <div>
                    <label for="confirm-password" class="form-label">Confirmar Senha</label>
                    <input type="password" class="form-control" id="confirm-password" name="confirm-password">
                </div>
                <div class="form-check">
                    <input class="form-check-input" type="checkbox" value="agree" id="agree" name="agree">
                    <label class="form-check-label" for="agree">
                      Eu li e concordo com os <a href="">Termos de Privacidade e Segurança</a>
                    </label>
                </div>
                <div class="d-grid">
                    <button class="btn btn-eboi btn-lg text-light" type="submit" style="height: 3em;">Cadastrar</button>
                </div>                  
            </div>
        </form>
    </main>
